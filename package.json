{
  "name": "react-mern",
  "version": "1.0.0",
  "description": "Dashboard access on the Web.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "project": "npm-run-all --parallel tab:*",
    "server": "nodemon server/server.js",
    "tab:server": "ttab -t 'Server' nodemon server/server.js",
    "tab:client": "ttab -t 'Client' npm start --prefix client",
    "tab:echo": "echo 'Starting project...'"
  },
  "author": "J.R. McCann",
  "license": "ISC",
  "dependencies": {
    "@johnrobertmcc/dashboard-library": "^0.0.2",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.0",
    "node-fetch": "^3.2.6",
    "ttab": "^0.7.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5"
  }
}
